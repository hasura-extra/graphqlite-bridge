{
    "name": "hasura-extra/graphqlite-bridge",
    "license": "MIT",
    "description": "Provides integration for GraphQLite with Hasura Extra packages.",
    "autoload": {
        "psr-4": {
            "Hasura\\GraphQLiteBridge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\GraphQLiteBridge\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "hasura-extra/api-client": "^3.1",
        "hasura-extra/metadata": "^3.1",
        "thecodingmachine/graphqlite": "^5.0",
        "symfony/uid": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "conflict": {
        "hasura-extra/sailor-bridge": "<3.0.0",
        "hasura-extra/auth-hook": "<3.0.0",
        "hasura-extra/event-dispatcher": "<3.0.0",
        "hasura-extra/bundle": "<3.0.0",
        "hasura-extra/laravel": "<2.5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
